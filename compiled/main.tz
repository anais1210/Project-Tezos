{ parameter (or (address %removeAdmin) (address %setAdmin)) ;
  storage (map address bool) ;
  code { UNPAIR ;
         IF_LEFT
           { NONE bool ; SWAP ; UPDATE }
           { DUP 2 ;
             DUP 2 ;
             GET ;
             IF_NONE
               { SWAP ; PUSH bool True ; DIG 2 ; SWAP ; SOME ; SWAP ; UPDATE }
               { DROP 3 ; PUSH string "Address already exist" ; FAILWITH } } ;
         NIL operation ;
         PAIR } }

